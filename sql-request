CREATE TABLE employees
(
id INTEGER PRIMARY KEY NOT NULL,
employee_id INTEGER NOT NULL UNIQUE,
first_name VARCHAR NOT NULL,
middle_name VARCHAR NOT NULL,
last_name VARCHAR NOT NULL
);

